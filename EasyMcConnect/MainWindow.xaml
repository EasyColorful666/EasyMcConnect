<Window
    x:Class="EasyMcConnect.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:ikw="http://schemas.inkore.net/lib/ui/wpf"
    xmlns:local="clr-namespace:EasyMcConnect"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="http://schemas.inkore.net/lib/ui/wpf/modern"
    Title="MainWindow"
    Width="800"
    Height="511"
    ui:ThemeManager.IsThemeAware="True"
    ui:WindowHelper.SystemBackdropType="Mica"
    ui:WindowHelper.UseModernWindowStyle="True"
    mc:Ignorable="d" Loaded="Window_Loaded">
    <TabControl x:Name="tabControl" IsSynchronizedWithCurrentItem="True" OverridesDefaultStyle="True">
        <TabItem Header="FRP">
            <Grid Background="#FFE5E5E5" Margin="2,0,-2,0" Height="464">
                <Button
                    x:Name="button1_frp"
                    Width="368"
                    Height="76"
                    Margin="24,238,0,0"
                    VerticalAlignment="Top"
                    Click="button1_frp_Click"
                    Content="Start"
                    Style="{StaticResource {x:Static ui:ThemeKeys.AccentButtonStyleKey}}" />
                <Button
                    x:Name="button2_frp"
                    Width="368"
                    Height="76"
                    Margin="24,318,0,0"
                    VerticalAlignment="Top"
                    Click="button2_frp_Click"
                    Content="Stop" />
                <TextBox
                    x:Name="textBox1_frp"
                    Width="290"
                    Height="42"
                    Margin="102,51,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    TextWrapping="Wrap" />
                <Label
                    x:Name="label复制__C_复制__C_"
                    Height="31"
                    Margin="4,121,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Content="协议(默认TCP)" />
                <ComboBox
                    x:Name="comboBox1_frp"
                    Width="290"
                    Margin="102,111,0,0" Height="41">
                    <ComboBoxItem Content="TCP" />
                    <ComboBoxItem Content="UDP" />
                    <ComboBoxItem Content="HTTP"/>
                </ComboBox>
                <TextBox
                    x:Name="textBox4_frp"
                    Width="385"
                    Height="331"
                    Margin="405,0,0,33"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Bottom"
                    Background="Gray"
                    Foreground="DarkGreen"
                    IsReadOnlyCaretVisible="True"
                    TextWrapping="Wrap" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Visible" />
                <TextBox
                    x:Name="textBox3_frp"
                    Width="290"
                    Height="40"
                    Margin="102,174,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    TextWrapping="Wrap" IsReadOnly="True" />
                <Label
                    x:Name="label复制__C_1复制__C_复制__C_复制__C_"
                    Height="31"
                    Margin="22,183,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Content="远程端口"
                    RenderTransformOrigin="0.488,0.657" />
                <Button x:Name="button3_frp" Content="清空输出" Margin="405,10,0,0" VerticalAlignment="Top" Height="60" Width="385" Click="button3_frp_Click"/>
                <Label
                    x:Name="label复制__C_1复制__C_复制__C_复制__C_复制__C_"
                    Height="31"
                    Margin="22,63,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Content="本地端口"
                    RenderTransformOrigin="0.488,0.657" />
            </Grid>
        </TabItem>
        <TabItem Height="30" Header="Easytier" Width="100">
            <Grid Background="#FFE5E5E5">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="29*"/>
                    <ColumnDefinition Width="771*"/>
                </Grid.ColumnDefinitions>
                <Button
                    x:Name="button1_et"
                    Width="368"
                    Height="76"
                    Margin="26,240,0,0"
                    VerticalAlignment="Top"
                    Content="Start"
                    Style="{StaticResource {x:Static ui:ThemeKeys.AccentButtonStyleKey}}" Click="button1_et_Click" Grid.ColumnSpan="2" />
                <Button
                    x:Name="button2_et"
                    Width="368"
                    Height="76"
                    Margin="26,322,0,0"
                    VerticalAlignment="Top"
                    Content="Stop" Click="button2_et_Click" Grid.ColumnSpan="2" />
                <TextBox
                    x:Name="textBox4_et"
                    Width="385"
                    Height="321"
                    Margin="376,0,0,37"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Bottom"
                    Background="Gray"
                    Foreground="DarkGreen"
                    IsReadOnlyCaretVisible="True"
                    TextWrapping="Wrap" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Visible" Grid.Column="1" />
                <Button x:Name="button3_et" Content="清空输出" Margin="376,10,0,0" VerticalAlignment="Top" Height="60" Width="385" Click="button3_et_Click" Grid.Column="1"/>
                <TextBox
                    x:Name="textBox3_et"
                    Width="292"
                    Height="42"
                    Margin="73,135,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    TextWrapping="Wrap" Grid.Column="1" />
                <Label
                    x:Name="label复制__C_1复制__C_复制__C_复制__C_复制__C_复制__C_"
                    Height="31"
                    Margin="24,147,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Content="本地ip"
                    RenderTransformOrigin="0.488,0.657" Grid.ColumnSpan="2" />
                <TextBox
                    x:Name="textBox1_et"
                    Width="290"
                    Height="24"
                    Margin="73,38,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    TextWrapping="Wrap" Grid.Column="1" />
                <Label
                    x:Name="label复制__C_2"
                    Height="24"
                    Margin="24,46,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Content="应用名称" Grid.ColumnSpan="2" />
                <TextBox
                    x:Name="textBol2_et"
                    Width="290"
                    Height="24"
                    Margin="73,90,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    TextWrapping="Wrap" Grid.Column="1" />
                <Label
                    x:Name="label复制__C_2复制__C_"
                    Height="24"
                    Margin="24,98,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Content="应用密码" Grid.ColumnSpan="2" />
            </Grid>
        </TabItem>
    </TabControl>
</Window>
