<Window
    x:Class="EasyMcConnect.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:ikw="http://schemas.inkore.net/lib/ui/wpf"
    xmlns:local="clr-namespace:EasyMcConnect"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="http://schemas.inkore.net/lib/ui/wpf/modern"
    Title="MainWindow"
    Width="800"
    Height="474"
    ui:ThemeManager.IsThemeAware="True"
    ui:WindowHelper.SystemBackdropType="Mica"
    ui:WindowHelper.UseModernWindowStyle="True"
    mc:Ignorable="d">
    <TabControl x:Name="tabControl" IsSynchronizedWithCurrentItem="True" OverridesDefaultStyle="True">
        <TabItem Header="P2P">
            <Grid Background="#FFE5E5E5">
                <Button
                    x:Name="button1"
                    Width="368"
                    Height="76"
                    Margin="24,260,0,0"
                    VerticalAlignment="Top"
                    Click="button1_Click"
                    Content="Start"
                    Style="{StaticResource {x:Static ui:ThemeKeys.AccentButtonStyleKey}}" />
                <Button
                    x:Name="button3"
                    Width="368"
                    Height="76"
                    Margin="24,341,0,0"
                    VerticalAlignment="Top"
                    Click="button3_Click"
                    Content="Stop" />
                <TextBox
                    x:Name="textBox1"
                    Width="290"
                    Height="24"
                    Margin="102,38,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    TextWrapping="Wrap" />
                <Label
                    x:Name="label"
                    Height="24"
                    Margin="24,46,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Content="应用名称" />
                <Label
                    x:Name="label复制__C_"
                    Height="24"
                    Margin="4,128,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Content="协议(默认TCP)" />
                <ComboBox
                    x:Name="comboBox1"
                    Width="290"
                    Margin="102,120,0,0">
                    <ComboBoxItem Content="TCP" />
                    <ComboBoxItem Content="UDP" />
                </ComboBox>
                <TextBox
                    x:Name="textBox2"
                    Width="290"
                    Height="24"
                    Margin="102,77,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    TextWrapping="Wrap" />
                <Label
                    x:Name="label复制__C_1"
                    Height="24"
                    Margin="38,85,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Content="端口" />
                <TextBox
                    x:Name="textBox4"
                    Width="385"
                    Height="340"
                    Margin="405,0,0,10"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Bottom"
                    Background="Gray"
                    Foreground="DarkGreen"
                    IsReadOnlyCaretVisible="True"
                    TextWrapping="Wrap" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Visible" />
                <TextBox
                    x:Name="textBox3"
                    Width="290"
                    Height="24"
                    Margin="102,160,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    TextWrapping="Wrap" />
                <Label
                    x:Name="label复制__C_1复制__C_"
                    Height="24"
                    Margin="10,168,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Content="目标电脑名字"
                    RenderTransformOrigin="0.488,0.657" />
                <TextBox
                    x:Name="textBox5"
                    Width="290"
                    Height="24"
                    Margin="102,200,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    IsReadOnly="True"
                    TextWrapping="Wrap" />
                <Label
                    x:Name="label复制__C_1复制__C_复制__C_"
                    Height="24"
                    Margin="22,208,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Content="本地端口"
                    RenderTransformOrigin="0.488,0.657" />
                <Button x:Name="button" Content="清空输出" Margin="404,0,0,0" VerticalAlignment="Top" Height="60" Width="188" Click="button_Click"/>
                <Button x:Name="button复制__C_" Content="启动P2P进程" Margin="598,0,0,0" VerticalAlignment="Top" Height="60" Width="192" Click="button复制__C__Click"/>
            </Grid>
        </TabItem>
        <TabItem Header="FRP">
            <Grid Background="#FFE5E5E5">
                <Button
                    x:Name="button1_frp"
                    Width="368"
                    Height="76"
                    Margin="24,260,0,0"
                    VerticalAlignment="Top"
                    Click="button1_frp_Click"
                    Content="Start"
                    Style="{StaticResource {x:Static ui:ThemeKeys.AccentButtonStyleKey}}" />
                <Button
                    x:Name="button2_frp"
                    Width="368"
                    Height="76"
                    Margin="24,341,0,0"
                    VerticalAlignment="Top"
                    Click="button2_frp_Click"
                    Content="Stop" />
                <TextBox
                    x:Name="textBox1_frp"
                    Width="290"
                    Height="42"
                    Margin="102,51,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    TextWrapping="Wrap" />
                <Label
                    x:Name="label复制__C_复制__C_"
                    Height="31"
                    Margin="4,121,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Content="协议(默认TCP)" />
                <ComboBox
                    x:Name="comboBox1_frp"
                    Width="290"
                    Margin="102,111,0,0" Height="41">
                    <ComboBoxItem Content="TCP" />
                    <ComboBoxItem Content="UDP" />
                </ComboBox>
                <TextBox
                    x:Name="textBox4_frp"
                    Width="385"
                    Height="340"
                    Margin="405,0,0,10"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Bottom"
                    Background="Gray"
                    Foreground="DarkGreen"
                    IsReadOnlyCaretVisible="True"
                    TextWrapping="Wrap" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Visible" />
                <TextBox
                    x:Name="textBox3_frp"
                    Width="290"
                    Height="40"
                    Margin="102,174,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    TextWrapping="Wrap" IsReadOnly="True" />
                <Label
                    x:Name="label复制__C_1复制__C_复制__C_复制__C_"
                    Height="31"
                    Margin="22,183,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Content="远程端口"
                    RenderTransformOrigin="0.488,0.657" />
                <Button x:Name="button3_frp" Content="清空输出" Margin="405,0,0,0" VerticalAlignment="Top" Height="60" Width="385" Click="button3_frp_Click"/>
                <Label
                    x:Name="label复制__C_1复制__C_复制__C_复制__C_复制__C_"
                    Height="31"
                    Margin="22,63,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Content="本地端口"
                    RenderTransformOrigin="0.488,0.657" />
            </Grid>
        </TabItem>
    </TabControl>
</Window>
